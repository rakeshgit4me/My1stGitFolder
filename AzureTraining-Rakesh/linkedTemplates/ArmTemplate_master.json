{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "AzureTraining-Rakesh"
		},
		"ABS_Rakesh_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ABS_Rakesh'"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"HttpServer_GitFolder_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'HttpServer_GitFolder'"
		},
		"Rakesh_AzureSqlDatabase_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Rakesh_AzureSqlDatabase'"
		},
		"SqlServerAzure_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SqlServerAzure'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storedata4me.dfs.core.windows.net/"
		},
		"HttpServer_GitFolder_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://github.com/rakeshgit4me"
		},
		"HttpServer_GitFolder_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "rakeshgit4me"
		},
		"Rakesh_AzureSqlDatabase_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "tesdataserver.database.windows.net"
		},
		"Rakesh_AzureSqlDatabase_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "tesdatabase"
		},
		"Rakesh_AzureSqlDatabase_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqladmin"
		},
		"SqlServerAzure_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "127.0.0.1"
		},
		"SqlServerAzure_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "ORG"
		},
		"SqlServerAzure_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sa"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "AzureTraining-Rakesh_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ABS_Rakesh_connectionString": {
						"value": "[parameters('ABS_Rakesh_connectionString')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"HttpServer_GitFolder_password": {
						"value": "[parameters('HttpServer_GitFolder_password')]"
					},
					"Rakesh_AzureSqlDatabase_password": {
						"value": "[parameters('Rakesh_AzureSqlDatabase_password')]"
					},
					"SqlServerAzure_password": {
						"value": "[parameters('SqlServerAzure_password')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"HttpServer_GitFolder_properties_typeProperties_url": {
						"value": "[parameters('HttpServer_GitFolder_properties_typeProperties_url')]"
					},
					"HttpServer_GitFolder_properties_typeProperties_userName": {
						"value": "[parameters('HttpServer_GitFolder_properties_typeProperties_userName')]"
					},
					"Rakesh_AzureSqlDatabase_properties_typeProperties_server": {
						"value": "[parameters('Rakesh_AzureSqlDatabase_properties_typeProperties_server')]"
					},
					"Rakesh_AzureSqlDatabase_properties_typeProperties_database": {
						"value": "[parameters('Rakesh_AzureSqlDatabase_properties_typeProperties_database')]"
					},
					"Rakesh_AzureSqlDatabase_properties_typeProperties_userName": {
						"value": "[parameters('Rakesh_AzureSqlDatabase_properties_typeProperties_userName')]"
					},
					"SqlServerAzure_properties_typeProperties_server": {
						"value": "[parameters('SqlServerAzure_properties_typeProperties_server')]"
					},
					"SqlServerAzure_properties_typeProperties_database": {
						"value": "[parameters('SqlServerAzure_properties_typeProperties_database')]"
					},
					"SqlServerAzure_properties_typeProperties_userName": {
						"value": "[parameters('SqlServerAzure_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}